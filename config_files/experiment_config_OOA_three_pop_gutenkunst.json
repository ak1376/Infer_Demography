{
  "genome_length": 1e7,
  "mutation_rate": 1.5e-08,
  "recombination_rate": 1.5e-08,
  "demographic_model": "OOA_three_pop_gutenkunst",
  "log_dir": "OOA_three_pop/inferences/moments/logs",
  "num_samples": {
    "YRI": 10,
    "CEU": 10,
    "CHB": 10
  },
  "seed": 42,
  "num_optimizations": 100,
  "top_k": 5,
  "num_draws": 5000,

  "priors": {
    "N_AFR_ancient": [3000, 20000],
    "N_AFR_recent":  [6000, 40000],

    "N_OOA":         [500, 12000],

    "N_CEU_founder": [200, 8000],
    "N_CHB_founder": [150, 6000],

    "r_CEU":         [0.0005, 0.012],
    "r_CHB":         [0.0005, 0.015],

    "T_OOA_EU_AS":          [400, 2000],
    "T_AFR_OOA":            [3500, 10000],
    "T_AFR_ancient_change": [6000, 16000],

    "m_YRI_OOA": [5e-5, 8e-4],
    "m_YRI_CEU": [1e-6, 3e-4],
    "m_YRI_CHB": [1e-6, 2e-4],
    "m_CEU_CHB": [5e-6, 6e-4]
  },


  "selection": {
    "enabled": true,
    "species": "HomSap",
    "dfe_id": "Gamma_K17",
    "coverage_percent": [0, 95],
    "slim_scaling": 10.0
  },

  "use_fim_features": true,
  "fim_engines": ["moments"],
  "use_residuals": false,
  "residual_engines": "moments",
  "engine": "msprime",
  "use_gpu_ld": true,
  "remove_exons": true,
  "gram_schmidt": true,
  "gram_schmidt_k": 10
}
